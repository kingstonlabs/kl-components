{
  "plugins": [
    "@babel/plugin-syntax-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    [
        "prismjs",
        {
            "languages": ["javascript", "css", "markup", "python", "bash"],
            "plugins": ["line-numbers"],
            "theme": "coy",
            "css": true,
        }
    ],
  ],
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
  ]
}
